        <ImageBrush.RelativeTransform>
            <TransformGroup>
                <ScaleTransform CenterY="0.5" CenterX="0.5" ScaleX="1.06" ScaleY="1.04"/>
                <SkewTransform CenterY="0.5" CenterX="0.5"/>
                <RotateTransform CenterY="0.5" CenterX="0.5"/>
                <TranslateTransform X="0.002"/>
            </TransformGroup>
        </ImageBrush.RelativeTransform>
    </ImageBrush>
    <ImageBrush x:Key="MenuFondBrush" ImageSource="/KTouch.Controls;component/Themes/Resources/KTouchMenuRes/menu_fond.png" Stretch="None"/>
    <ImageBrush x:Key="ItemOffBackgroundBrush" ImageSource="/KTouch.Controls;component/Themes/Resources/KTouchBandeRes/vignette_OFF2.png" Stretch="Uniform"/>
